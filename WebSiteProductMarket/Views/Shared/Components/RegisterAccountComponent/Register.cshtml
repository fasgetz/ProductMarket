@model WebSiteProductMarket.Models.ViewModels.Account.RegisterUserViewModel

    <h2>Регистрация пользователя</h2>


    <form asp-action="Register" asp-controller="Account" asp-route-returnUrl="@Model?.ReturnUrl" data-ajax="true"
          data-ajax-method="POST"
          data-ajax-mode="replace" data-ajax-update="#mainForm"
          data-ajax-success="success">
        <div id="mainForm">
            @await Html.PartialAsync("Controllers/Account/Register")
        </div>

    </form>

